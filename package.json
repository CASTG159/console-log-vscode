{
    "name": "console-log",
    "displayName": "Console Log",
    "description": "Create console.log lines faster",
    "version": "0.0.3",
    "publisher": "Luis",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.clog"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.clog",
                "title": "Console.log"
            }
        ],
        "keybindings": [
            {
                "mac": "alt+-",
                "win": "alt+-",
                "linux": "alt+-",
                "key": "alt+-",
                "when": "editorTextFocus",
                "command": "extension.clog"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LuisEGR/console-log-vscode.git"
    }
}
